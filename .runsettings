<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<RunConfiguration>
		<ResultsDirectory>./TestResults</ResultsDirectory>
	</RunConfiguration>
	<DataCollectionRunSettings>
		<DataCollectors>
			<DataCollector friendlyName="XPlat Code Coverage">
				<Configuration>
					<Format>cobertura</Format>
					<Exclude>[*.Tests]*;[*]*.Program;[*]*.Startup</Exclude>
					<ExcludeByFile>**/obj/**/*.cs;**/bin/**/*.cs;**/obj/**;**/bin/**;**/*.g.cs;**/*.Designer.cs;**\obj\**\*.cs;**\bin\**\*.cs</ExcludeByFile>
					<ExcludeByAttribute>Obsolete,GeneratedCodeAttribute,CompilerGeneratedAttribute,ExcludeFromCodeCoverageAttribute</ExcludeByAttribute>
					<SkipAutoProps>true</SkipAutoProps>
					<DeterministicReport>true</DeterministicReport>
					<SingleHit>false</SingleHit>
					<IncludeTestAssembly>false</IncludeTestAssembly>
				</Configuration>
			</DataCollector>
		</DataCollectors>
	</DataCollectionRunSettings>
	<CodeCoverage>
		<Filters>
			<Include>
				<Module>^CommonNetFuncs\..*</Module>
			</Include>
			<Exclude>
				<File>.*\.g\.cs$</File>
				<File>.*\.Designer\.cs$</File>
				<Path>.*\\obj\\.*</Path>
				<Path>.*\\bin\\.*</Path>
				<Path>.*/obj/.*</Path>
				<Path>.*/bin/.*</Path>
				<Path>*\\obj\\*</Path>
				<Path>*\\bin\\*</Path>
				<Path>*/obj/*</Path>
				<Path>*/bin/*</Path>
				<Source>.*\\obj\\.*</Source>
				<Source>.*\\bin\\.*</Source>
				<Source>.*/obj/.*</Source>
				<Source>.*/bin/.*</Source>
				<Source>*\\obj\\*</Source>
				<Source>*\\bin\\*</Source>
				<Source>*/obj/*</Source>
				<Source>*/bin/*</Source>
			</Exclude>
		</Filters>
		<ModulePaths>
			<Exclude>
				<ModulePath>.*\\obj\\.*</ModulePath>
				<ModulePath>.*\\bin\\.*</ModulePath>
				<ModulePath>.*/obj/.*</ModulePath>
				<ModulePath>.*/bin/.*</ModulePath>
				<ModulePath>.*\\obj\\*.cs</ModulePath>
				<ModulePath>.*\\bin\\*.cs</ModulePath>
				<ModulePath>.*/obj/*.cs</ModulePath>
				<ModulePath>.*/bin/*.cs</ModulePath>
				<ModulePath>.*\\obj\\*.dll</ModulePath>
				<ModulePath>.*\\bin\\*.dll</ModulePath>
				<ModulePath>.*/obj/*.dll</ModulePath>
				<ModulePath>.*/bin/*.dll</ModulePath>
				<ModulePath>.*Tests.dll</ModulePath>
				<ModulePath>.*AutoGenerated.dll</ModulePath>
			</Exclude>
		</ModulePaths>
	</CodeCoverage>
</RunSettings>